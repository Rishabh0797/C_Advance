#include <stdio.h>

int main()
{
    printf("Enter you number :");
    int i,j,num,a;                                    //if num is 5......
    scanf("%d",&num);                                 //5
    a=0;                                              //54
    for(i=num;i>=0;i--)                               //543
    {                                                 //5432
        for(j=0;j<=a;j++)                             //54321
        {                                             //543210......
            printf("%d",num-j);
        }
        a++;
        printf("\n");
    }
    return 0;
}
