#include <stdio.h>

int main()
{
    int i,j,num;
    printf("Enter number :");
    scanf("%d",&num);                         //00000
    for(i=0;i<=num;i++)                       //11111
    {                                         //22222
        printf("\n");                         //33333
        for(j=0;j<=5;j++)                     //44444
        {                                     //55555....
            printf("%d",i);
        }
    }

    return 0;
}
