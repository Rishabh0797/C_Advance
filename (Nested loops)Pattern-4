#include <stdio.h>

int main()
{
    printf("Enter your number :");                   //12345
    int i,j,num;                                     //1234
    scanf("%d",&num);                                //123
    for(i=num;i>=0;i--)                              //12
    {                                                //1......
        for(j=1;j<=i;j++)
        {
            printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}
