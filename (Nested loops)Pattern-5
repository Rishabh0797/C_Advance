#include <stdio.h>

int main()
{
    printf("Enter your number :");                     //12345
    int num,i,j;                                       //2345
    scanf("%d",&num);                                  //345
    for(i=1;i<=num;i++)                                //45
    {                                                  //5.......
        for(j=i;j<=num;j++)
        {
            printf("%d",j);
        }
        printf("\n");
    }
    return 0;
}
